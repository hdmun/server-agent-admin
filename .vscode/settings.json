{
    "files.exclude": {
        "**/.nuxt": true,
        "**/dist": true,
        "**/node_modules": true,
        "**/storybook-static": true,
        "**/.nuxt-storybook": true
    },
    "[vue]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "Vue.volar"
    },
    "emmet.includeLanguages": {
        "vue-html": "html",
        "vue": "html"
    },
    "js/ts.implicitProjectConfig.experimentalDecorators": true,
    "editor.tabSize": 2,
}